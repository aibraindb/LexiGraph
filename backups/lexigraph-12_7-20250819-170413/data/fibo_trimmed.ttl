@prefix fibo: <https://spec.edmcouncil.org/fibo/ontology/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

fibo:LOANS/Loan a rdfs:Class ; rdfs:label "loan" .
fibo:LEASING/LeaseAgreement a rdfs:Class ; rdfs:label "lease agreement" .
fibo:INVOICE/Invoice a rdfs:Class ; rdfs:label "invoice" .

# Simple properties to demo attribute extraction
fibo:prop/borrower a owl:ObjectProperty ; rdfs:label "borrower" ; rdfs:domain fibo:LOANS/Loan .
fibo:prop/principal a owl:DatatypeProperty ; rdfs:label "principal amount" ; rdfs:domain fibo:LOANS/Loan .
fibo:prop/interestRate a owl:DatatypeProperty ; rdfs:label "interest rate" ; rdfs:domain fibo:LOANS/Loan .

fibo:prop/lessee a owl:ObjectProperty ; rdfs:label "lessee" ; rdfs:domain fibo:LEASING/LeaseAgreement .
fibo:prop/lessor a owl:ObjectProperty ; rdfs:label "lessor" ; rdfs:domain fibo:LEASING/LeaseAgreement .
fibo:prop/term a owl:DatatypeProperty ; rdfs:label "term" ; rdfs:domain fibo:LEASING/LeaseAgreement .

fibo:prop/vendor a owl:ObjectProperty ; rdfs:label "vendor" ; rdfs:domain fibo:INVOICE/Invoice .
fibo:prop/total a owl:DatatypeProperty ; rdfs:label "total amount" ; rdfs:domain fibo:INVOICE/Invoice .
